{
    "presets": ["es2015", "stage-1", "react"],
    "plugins": ["transform-decorators-legacy"],
    "env": {
      "development": {
        "plugins": [
          ["react-transform", {
              "transforms": [{
                "transform": "react-transform-hmr",
                "imports": ["react"],
                "locals": ["module"]
              }]
          }]
        ]
      }
    }
}
